{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@front/*": ["../front_end/src/*"],
      "@utils/*": ["../front_end/src/utils/*"],
      "@shared/*": ["../shared/*"],
      "@theme/*": ["theme/*"],
      "@lib/*": ["lib/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules", "dist"]
}
