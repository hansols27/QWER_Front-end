{
  "version": 2,
  "builds": [
    { 
      "src": "dist/index.js", 
      "use": "@vercel/node" 
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "https://qwer-fansite-admin.vercel.app, https://qwer-fansite-front.vercel.app",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
      }
    }
  ]
}