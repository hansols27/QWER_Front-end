{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@front/*": ["src/*"],
      "@app/*": ["src/app/*"],
      "@components/*": ["src/components/*"],
      "@utils/*": ["src/utils/*"],
      "@shared/*": ["../shared/*"],
      "@theme/*": ["theme/*"],
      "@lib/*": ["lib/*"]
    }
  },
  "include": ["src/**/*"]
}
